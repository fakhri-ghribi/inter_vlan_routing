{% for int in interfaces_trunk %}
  interface {{int['name']}}
    description {{in['desc']}}
    switchport mode {{int['mode']}}
    {% for vl in int['allowed_vlans'] %}
      switchport trunk allowed vlan add {{vl['id']}}
    {% endfor %}
!
{% endfor %}

ip routing
router ospf {{ospf_process}}
  router-id {{router_id}}
  {% for net in net_ospf %}
    network {{net['net']}} {{net['wildcard']}} area {{ospf_area}}
    